@misc{santurkarWhoseOpinionsLanguage2023,
  title = {Whose {{Opinions Do Language Models Reflect}}?},
  author = {Santurkar, Shibani and Durmus, Esin and Ladhak, Faisal and Lee, Cinoo and Liang, Percy and Hashimoto, Tatsunori},
